@startuml db-schema

class User {
    FirstName: string
    LastName: string
    Email: string
    Password: string
    Role: enum
}

class Log {
    UserId: int
    LogDate: datetime
}

class Technology {
    Name: string
    Description: string
    Category: enum
    Ring: enum
    RingReason: string
    CreatedDate: datetime
    UpdatedDate: datetime
    Published: boolean
    PublishedDate: datetime
}

@enduml